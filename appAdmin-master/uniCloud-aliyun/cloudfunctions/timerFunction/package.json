{
    "name": "timerFunction",
    "main": "index.js",
    "version": "1.0.0",
    "cloudfunction-config": {
        "concurrency": 1,
        "memorySize": 256,
        "timeout": 600,
        "runtime": "Nodejs12",
        "triggers": [
            {
                "name": "myTrigger",
                "type": "timer",
                "config": "0 1 * * * * *"
            }
        ],
        "path": ""
    }
}