// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
	"bsonType": "object",
	"required": ["uid","score","nickname"],
	"permission": {
		"read": false,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"uid": {
			"description": "用户ID",
			"bsonType": "string"
		},
		"nickname": {
			"description": "用户昵称",
			"bsonType": "string"
		},
		"score":{
			"title": "分数",
			"bsonType": "string"
		},
		"_creatTime":{
			"title": "创建时间",
			"bsonType": "timestamp",
			"forceDefaultValue":{
				"$env": "now"
			}
		}
		
		
	}
}